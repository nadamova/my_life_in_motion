                                        <span>
                                            <cufontext>{{pricesixlow}} lv</cufontext>
                                        </span>
                                </span>
                            </a>
                        </div>
                        <div class="menu-aside">
                            <h5>{{this.model}}</h5>
                            <p>
                                <span class="capacity">For: {{this.capacity}} people</span>
                            </p>
                        </div>
                    </div>
                </div>
                {{/each}}
            </div>
        </div>
        <div class="col-xs-12 col-sm-6 col-md-4 category-elemets">
            <h3 class="category-title">
                Semi-integrated
            </h3>
            <div class="category-inner-element">
                {{#each campers.semi-integrated}}
                <div class="thumbnail">
                    <div class="row">
                        <div class="col-md-3">
                            <a href="#/camper/{{this._id}}">
